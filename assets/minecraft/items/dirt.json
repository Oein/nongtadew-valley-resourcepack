{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "block/dirt"
    },
    "cases": [
      {
        "when": "carrot_seed",
        "model": {
          "type": "model",
          "model": "item/carrot/seed"
        }
      },
      {
        "when": "carrot_growing",
        "model": {
          "type": "model",
          "model": "item/carrot/growing"
        }
      },
      {
        "when": "carrot_grown",
        "model": {
          "type": "model",
          "model": "item/carrot/grown"
        }
      },
      {
        "when": "carrot_shit",
        "model": {
          "type": "model",
          "model": "item/carrot/shit"
        }
      },

      {
        "when": "sugarcane_seed",
        "model": {
          "type": "model",
          "model": "item/sugarcane/seed"
        }
      },
      {
        "when": "sugarcane_growing",
        "model": {
          "type": "model",
          "model": "item/sugarcane/growing"
        }
      },
      {
        "when": "sugarcane_grown",
        "model": {
          "type": "model",
          "model": "item/sugarcane/grown"
        }
      },
      {
        "when": "sugarcane_shit",
        "model": {
          "type": "model",
          "model": "item/sugarcane/shit"
        }
      }
    ]
  }
}
